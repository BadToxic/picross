// Initialize empty game area
var xx = game_area_x2, yy;
for (var field_index_x = fields_x - 1; field_index_x &gt;= 0; field_index_x--) {
    yy = game_area_y2;
    
    for (var field_index_y = fields_y - 1; field_index_y &gt;= 0; field_index_y--) {
    
        var field_state = game_field[field_index_y, field_index_x];
        
        var x1 = xx - field_size + 1;
        var y1 = yy - field_size + 1;
        var x2 = xx - 1;
        var y2 = yy - 1;
        
        var mouse_index = 0;
        if (device_mouse_x(mouse_index) &gt;= x1) {
            if (device_mouse_y(mouse_index) &gt;= y1) {
                if (device_mouse_x(mouse_index) &lt; x2) {
                    if (device_mouse_y(mouse_index) &lt; y2) {
                        if (device_mouse_check_button_pressed(mouse_index, mb_left)) {
                            if (field_state == 1) {
                                field_state = 0;
                            } else {
                                field_state = 1;
                            }
                        } else if (device_mouse_check_button_pressed(mouse_index, mb_right)) {
                            if (field_state == -1) {
                                field_state = 0;
                            } else {
                                field_state = -1;
                            }
                        }
                    }
                }
            }
        }
        game_field[field_index_y, field_index_x] = field_state;
        
        if (field_state == 1) {
            draw_set_alpha(0.8);
            draw_roundrect_colour(x1, y1, x2, y2, c_aqua, c_blue, false);
        } else if (field_state == -1) {
            draw_set_alpha(0.6);
            draw_roundrect_colour(x1, y1, x2, y2, c_black, c_white, false);
        }
        draw_set_alpha(1);
        
        draw_set_color(c_black);
        draw_rectangle(x1, y1, x2, y2, true);
        
        yy -= field_size;
    }
    xx -= field_size;
}
</string>
